{
  "name": "linux-vqq-fix",
  "description": "修复Linux下Chrome浏览器无法播放腾讯视频",
  "version": "0.0.0.1",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://v.qq.com/*",
    "ws://v.qq.com/*",
    "wss://v.qq.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "js": [
        "contentscript.js"
      ],
      "matches": [
        "*://v.qq.com/*"
      ]
    }
  ],
  "icons": {
    "48": "fedora_48.png",
    "128": "fedora_128.png"
  },
  "manifest_version": 2,
  "author": "老雷, leizongmin@gmail.com"
}